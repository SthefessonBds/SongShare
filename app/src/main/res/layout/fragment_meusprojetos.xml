<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="?attr/colorButtonNormal"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/frame_meusprojetos"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:background="@android:color/background_light">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Biblioteca"
            android:textAlignment="center"
            android:textColor="#000"
            android:textSize="23sp"
            android:padding="10dp"/>

    </FrameLayout>

    <FrameLayout
        android:layout_marginTop="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:background="@android:color/background_light">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Meus Projetos"
            android:textSize="23sp"
            android:padding="10dp"/>

    </FrameLayout>

    <FrameLayout
        android:layout_marginTop="0dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:background="@android:color/background_light">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Colaborações"
            android:textSize="23sp"
            android:padding="10dp"/>

    </FrameLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Todos os projetos"
        android:textSize="19sp"
        android:padding="10dp"/>

        <FrameLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_alignParentTop="true"
    android:layout_marginTop="0dp"
    android:background="@android:color/background_light">

    <LinearLayout
        android:layout_width="380dp"
        android:layout_height="60dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            app:srcCompat="@drawable/userlogin" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="9dp"
            android:text="Entrada de Informática"
            android:textColor="#000"
            android:textSize="17sp"/>
        <ImageView

            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="70dp"
            android:layout_marginTop="10dp"
            app:srcCompat="@drawable/ic_sort_black_24dp" />

    </LinearLayout>

</FrameLayout>


    <FrameLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="3dp"
        android:background="@android:color/background_light">

        <LinearLayout
            android:layout_width="380dp"
            android:layout_height="60dp"
            android:orientation="horizontal">

            <ImageView

                android:layout_width="50dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                app:srcCompat="@drawable/userlogin" />

            <TextView

                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="12dp"
                android:layout_marginTop="9dp"
                android:text="Dança de Informática"
                android:textColor="#000"
                android:textSize="17sp"/>
            <ImageView

                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginLeft="70dp"
                android:layout_marginTop="10dp"
                app:srcCompat="@drawable/ic_sort_black_24dp" />

        </LinearLayout>

    </FrameLayout>

</LinearLayout>

